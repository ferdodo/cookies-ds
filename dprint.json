{
  "lineWidth": 160,
  "typescript": {
    "quoteStyle": "preferDouble",
    "useTabs": true,
    "trailingCommas": "never",
    "importDeclaration.sortNamedImports": "maintain",
    "module.sortImportDeclarations": "maintain"
  },
  "includes": [
    "**/*.{ts}"
  ],
  "excludes": [
    "**/node_modules"
  ],
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.81.1.wasm"
  ]
}
