{
	"compilerOptions": {
		"target": "es6",
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noImplicitAny": false,
		"skipLibCheck": true,
		"strict": true,
		"moduleResolution": "nodenext",
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"module": "esnext",
		"lib": ["dom", "es2018"],
		"outDir": "dist",
		"declaration": true,
		"allowJs": true
	}
}
